#!/usr/bin/env bash
if [ ! -f "${SNAP_USER_COMMON}/JDownloader.jar" ]; then
    cp "${SNAP}/usr/bin/JDownloader.jar" "${SNAP_USER_COMMON}/JDownloader.jar"
fi
export JAVA_HOME="${SNAP}/usr/lib/jvm/default-java"
export PATH="${PATH}:${JAVA_HOME}/bin"
java -Dsun.jnu.encoding=UTF-8 -Dfile.encoding=UTF-8 -jar "${SNAP_USER_COMMON}/JDownloader.jar"
